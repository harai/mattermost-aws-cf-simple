{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root",
    "logfile": "/var/log/cwagent.log"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/cfn-init.log",
            "log_group_name": "${CFNINIT_LOG}",
            "log_stream_name": "{instance_id}",
            "multi_line_start_pattern": "{timestamp_format}",
            "timestamp_format": "%Y-%m-%d %H:%M:%S"
          },
          {
            "file_path": "/var/log/mattermost/mattermost.log*",
            "log_group_name": "${MATTERMOST_LOG}",
            "log_stream_name": "{instance_id}",
            "multi_line_start_pattern": "{timestamp_format}",
            "timestamp_format": "\"timestamp\":\"%Y-%m-%d %H:%M:%S.%f Z\"",
            "timezone": "UTC"
          },
          {
            "file_path": "/var/log/mysql/error.log*",
            "log_group_name": "${MYSQL_ERROR_LOG}",
            "log_stream_name": "{instance_id}",
            "multi_line_start_pattern": "{timestamp_format}",
            "timestamp_format": "%Y-%m-%d %H:%M:%S"
          }
        ]
      }
    }
  }
}
